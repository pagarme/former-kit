{
  "name": "former-kit",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pagarme/former-kit.git"
  },
  "homepage": "https://github.com/pagarme/former-kit",
  "bugs": "https://github.com/pagarme/former-kit/issues",
  "scripts": {
    "build": "env NODE_ENV=production babel src $WATCH -d dist --ignore spec.js,test.js",
    "start": "env WATCH=-w run-p lint:watch build",
    "lint:watch": "chokidar './{src,stories}/**/*.{js,css}' -c 'run-s lint'",
    "lint": "eslint './{src,stories}/**/*.js' && stylelint './{src,stories}/**/*.css'",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage",
    "coverage": "nyc --reporter=html --reporter=text --lines 300 npm run test-coverage",
    "percy": "build-storybook && percy-storybook --widths=1024,1920; true",
    "docs:publish": "run-s docs:build; gh-pages -d docs",
    "docs:build": "rm -rf docs; run-p storybook:build styleguide:build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o ./docs/storybook",
    "styleguide": "styleguidist server --config=./config/styleguide.config.js",
    "styleguide:build": "styleguidist build --config=./config/styleguide.config.js",
    "prebuild": "rimraf ./dist",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "emblematic-icons": "0.6.1",
    "former-kit-skin-pagarme": "1.1.0",
    "moment": "2.24.0",
    "promise": "8.0.2",
    "prop-types": "15.6.2",
    "ramda": "0.26.1",
    "react": "16.7.0",
    "react-click-outside": "3.0.1",
    "react-dates": "18.4.1",
    "react-dom": "16.7.0",
    "react-input-mask": "^2.0.4",
    "react-maskedinput": "4.0.1",
    "react-modal": "3.8.1",
    "react-motion": "0.5.2",
    "react-with-direction": "1.3.0",
    "shortid": "2.2.14",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.2.0",
    "@percy-io/in-percy": "0.1.11",
    "@percy-io/percy-storybook": "2.1.0",
    "@storybook/addon-actions": "4.1.7",
    "@storybook/addon-links": "4.1.7",
    "@storybook/addon-storyshots": "4.1.7",
    "@storybook/addons": "4.1.7",
    "@storybook/channels": "4.1.7",
    "@storybook/react": "4.1.7",
    "@storybook/storybook-deployer": "2.8.1",
    "@svgr/webpack": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "caniuse-lite": "~1.0.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.3.0",
    "chokidar-cli": "1.2.0",
    "css-loader": "2.1.0",
    "dotenv": "6.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "4.16.0",
    "eslint-config-pagarme-react": "1.4.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "3.2.0",
    "intl": "1.2.5",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-in-case": "1.0.2",
    "mockdate": "2.0.2",
    "npm-run-all": "4.1.2",
    "nyc": "13.1.0",
    "object-assign": "4.1.1",
    "postcss-cssnext": "3.1.0",
    "postcss-hexrgba": "1.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-sass-each": "0.1.0",
    "postcss-url": "8.0.0",
    "react-dev-utils": "7.0.1",
    "react-error-overlay": "5.1.2",
    "react-moment-proptypes": "1.5.0",
    "react-styleguidist": "8.0.6",
    "react-test-renderer": "16.7.0",
    "react-testing-library": "5.4.4",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "stylelint": "9.10.0",
    "stylelint-config-pagarme-react": "1.2.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4"
  }
}

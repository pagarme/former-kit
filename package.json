{
  "name": "former-kit",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pagarme/former-kit.git"
  },
  "homepage": "https://github.com/pagarme/former-kit",
  "bugs": "https://github.com/pagarme/former-kit/issues",
  "scripts": {
    "build": "env NODE_ENV=production babel src $WATCH -d dist --ignore spec.js,test.js",
    "start": "env WATCH=-w run-p lint:watch build",
    "lint:watch": "chokidar './{src,stories}/**/*.{js,css}' -c 'run-s lint'",
    "lint": "eslint './{src,stories}/**/*.js' && stylelint './{src,stories}/**/*.css'",
    "test": "node scripts/test.js --env=jsdom",
    "test-coverage": "node scripts/test.js --env=jsdom --coverage",
    "coverage": "nyc --reporter=html --reporter=text --lines 300 npm run test-coverage",
    "percy": "build-storybook && percy-storybook --widths=1024,1920; true",
    "docs:publish": "run-s docs:build; gh-pages -d docs",
    "docs:build": "rm -rf docs; run-p storybook:build styleguide:build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o ./docs/storybook",
    "styleguide": "styleguidist server --config=./config/styleguide.config.js",
    "styleguide:build": "styleguidist build --config=./config/styleguide.config.js"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "moment": "2.20.1",
    "ramda": "0.25.0",
    "react-addons-shallow-compare": "15.6.2",
    "react-click-outside": "3.0.0",
    "react-dates": "12.7.1",
    "react-maskedinput": "4.0.1",
    "react-modal": "3.1.11",
    "react-motion": "0.5.2",
    "shortid": "2.2.8"
  },
  "devDependencies": {
    "@percy-io/in-percy": "0.1.10",
    "@percy-io/percy-storybook": "1.3.2",
    "@storybook/addon-actions": "3.3.11",
    "@storybook/addon-links": "3.3.11",
    "@storybook/addon-storyshots": "3.3.11",
    "@storybook/addons": "3.3.11",
    "@storybook/channels": "3.3.11",
    "@storybook/react": "3.3.11",
    "@storybook/storybook-deployer": "2.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-react-app": "3.1.1",
    "babel-runtime": "6.26.0",
    "caniuse-lite": "~1.0.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "2.3.0",
    "chokidar-cli": "1.2.0",
    "css-loader": "0.28.9",
    "dotenv": "4.0.0",
    "emblematic-icons": "0.2.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.16.0",
    "eslint-config-pagarme-react": "1.4.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "former-kit-skin-pagarme": "0.1.0",
    "fs-extra": "5.0.0",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "2.30.1",
    "jest": "22.1.4",
    "jest-cli": "22.1.4",
    "jest-in-case": "1.0.2",
    "mockdate": "2.0.2",
    "npm-run-all": "4.1.2",
    "nyc": "11.4.1",
    "object-assign": "4.1.1",
    "postcss-cssnext": "3.1.0",
    "postcss-hexrgba": "1.0.0",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.10",
    "postcss-sass-each": "0.1.0",
    "postcss-url": "7.3.0",
    "promise": "8.0.1",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dev-utils": "5.0.0",
    "react-dom": "16.2.0",
    "react-error-overlay": "4.0.0",
    "react-moment-proptypes": "1.5.0",
    "react-styleguidist": "6.2.1",
    "react-test-renderer": "16.2.0",
    "style-loader": "0.20.1",
    "stylelint": "8.4.0",
    "stylelint-config-pagarme-react": "1.2.0",
    "svgr": "1.6.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  }
}
